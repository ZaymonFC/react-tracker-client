{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "only-arrow-functions": true,
        "semicolon": [true, "never"],
        "indent": [true, "spaces", 2],
        "space-before-function-paren": true,
        "quotemark": [true, "single", "jsx-double"],
        "class-name": true,
        "no-empty-interface": false,
        "member-access": false
    },
    "rulesDirectory": []
}